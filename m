     upvar #0 viewperm current_viewperm
     upvar #0 nextviewnum current_nextviewnum
     upvar #0 use_ttk current_use_ttk
+    upvar #0 geometry current_geometry
 
     if {$stuffsaved} return
     if {![winfo viewable .]} return
